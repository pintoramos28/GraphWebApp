{
  "name": "graph-web-app",
  "version": "0.1.0",
  "private": true,
  "description": "Interactive JMP-like graph builder for engineers (scatter, line, bar) built with Next.js, Vega-Lite, and DuckDB-WASM.",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "test": "pnpm run test:unit",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "vega": "^6.2.0",
    "vega-embed": "^7.1.0",
    "vega-lite": "^6.4.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/react": "15.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.12.8",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-testing-library": "6.2.1",
    "jsdom": "24.0.0",
    "playwright": "1.43.1",
    "prettier": "3.2.5",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "packageManager": "pnpm@9.1.1"
}
